class Solution {
public:
    int numJewelsInStones(string jewels, string stones) {
        int j=jewels.size();
        int s=stones.size();
        int no=0;
        for(auto i:jewels){
            for(auto j:stones){
                if(i==j){
                    no++;
                }
            }
        }
        return no;
    }
};
