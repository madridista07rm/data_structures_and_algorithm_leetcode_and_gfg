class Solution {
public:
    string generateTheString(int n) {
      string s;
      if(n%2!=0){
          while(n>0){
              s+="a";
              n--;
          }
      }  
       else{
           while(n>1){
               s+="a";
               n--;
           }
           s+="b";
       } 
        return s;
    }
};
